<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="0" y="1.5" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.35" sy="0.35" sz="0.35" />
    <reference length="5" />
  </INITIALS>


  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.6" g="0.6" b="0.6" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>


  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light">
      <enable value ="1" />
      <position  x="7.5" y="10" z="9.5" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="0.7" g="0.7" b="0.7" a="1.0" />
      <specular r="0.7" g="0.7" b="0.7" a="1.0" />
    </LIGHT>

  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>
    <!-- Rug white texture -->
    <TEXTURE id="rug_new">
      <file path="images/rug_new2.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Floor lighter wood texture -->
    <TEXTURE id="floor_wood_lighter">
      <file path="images/floor_wood_lighter.jpg" />
      <amplif_factor s="0.2" t="0.1" />
    </TEXTURE>

     <!-- Outside texture -->
    <TEXTURE id="outside">
      <file path="images/outside.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

     <!-- Wallpaper texture -->
    <TEXTURE id="wallpaper">
      <file path="images/wallpaper.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Dark wood texture 2 -->
    <TEXTURE id="dark_wood2">
      <file path="images/dark_wood2.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="normal_wood3">
      <file path="images/normal_wood3.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Normal wood texture 4 -->
    <TEXTURE id="normal_wood4">
      <file path="images/normal_wood4.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Sheet texture -->
    <TEXTURE id="sheet">
      <file path="images/sheet.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Pillow texture -->
    <TEXTURE id="pillow">
      <file path="images/pillow.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Laptop texture -->
