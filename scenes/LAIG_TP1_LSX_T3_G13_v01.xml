      <file path="images/laptop.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Laptop parts texture -->
    <TEXTURE id="laptop_parts">
      <file path="images/laptop_parts.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

     <!-- Laptop front texture -->
    <TEXTURE id="laptop_front">
      <file path="images/laptop_front.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Table legs texture -->
    <TEXTURE id="table_legs">
      <file path="images/table_legs.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Paper texture -->
    <TEXTURE id="paper">
      <file path="images/paper.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Debug texture  texture -->
